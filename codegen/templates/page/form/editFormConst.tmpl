// editFormConst.tmpl

{{
// {{= title }}EditFormPath specifies the URL that will access this form.
const {{= title }}EditFormPath = "/goradd/forms/{{= dd.DbKey }}/{{= tableKebab }}-edit"
// {{= title }}EditFormID is the id of the html form object.
const {{= title }}EditFormID = "{{= tableKebab }}-edit-form"

// {{= title }}EditPanelID is the id of the html panel object that contains the content of the edit form.
const {{= title }}EditPanelID = "{{= t.DefaultHtmlID()}}-edit-panel"

// These constants are the ids of the buttons that appear on the bottom of the edit panel
const (
    {{= title }}SaveButtonID = "save-button"
    {{= title }}CancelButtonID = "cancel-button"
    {{= title }}DeleteButtonID = "delete-button"
)

// These constants are the action values that are triggered by the edit panel buttons.
const (
    {{= title }}SaveAction = iota + 1
    {{= title }}CancelAction
    {{= title }}DeleteAction
)

}}